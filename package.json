    "build": "./node_modules/typescript/bin/tsc --project tsconfig.prod.json",
    "build:test": "./node_modules/typescript/bin/tsc --noEmit",
    "build:test:prod": "./node_modules/typescript/bin/tsc --project tsconfig.prod.json --noEmit",
